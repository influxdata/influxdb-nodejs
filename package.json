{
  "name": "influxdb-nodejs",
  "version": "1.0.0",
  "description": "Node.js client for InfluxDB",
  "main": "./lib/index.js",
  "directories": {
    "doc": "doc",
    "test": "test",
    "lib": "lib"
  },
  "files": [
    "lib",
    "doc"
  ],
  "scripts": {
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "doc:watch": "esdoc",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "coverage": "cross-env babel-istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://dubsky@bitbucket.org/dubsky/influxdb-nodejs.git"
  },
  "keywords": [
    "influxdb"
  ],
  "author": "Vladimir Dubsky",
  "license": "ISC",
  "homepage": "https://bitbucket.org/dubsky/influxdb-nodejs#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "esdoc-node": "^1.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "stream-buffers": "^3.0.1"
  }
}
